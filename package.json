{
  "version": "0.3.0",
  "license": "MIT",
  "author": "KODE frontend team",
  "homepage": "https://github.com/appKODE/pathfinder-web-react",
  "repository": {
    "url": "git@https://github.com/appKODE/pathfinder-web-react",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/appKODE/pathfinder-web-react/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "yarn link && tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "release": "release-it",
    "prepare": "yarn unlink && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@types/react": ">=16",
    "@types/react-dom": ">=16",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@kode-frontend/pathfinder-web-react",
  "module": "dist/mytslib.esm.js",
  "size-limit": [
    {
      "path": "dist/mytslib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mytslib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@release-it/conventional-changelog": "^4.3.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/styled-components": "^5.1.25",
    "babel-loader": "^8.2.4",
    "babel-plugin-styled-components": "^2.0.7",
    "husky": "^7.0.4",
    "plop": "^3.1.0",
    "react-is": "^18.0.0",
    "release-it": "^14.14.2",
    "size-limit": "^7.0.8",
    "tailwindcss": "^3.0.24",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@kode-frontend/pathfinder-web-core": "^0.0.11",
    "fetch-intercept": "^2.4.0",
    "rollup-plugin-postcss": "3.1.6",
    "styled-components": "^5.3.5"
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "issueUrlFormat": "https://github.com/appKODE/pathfinder-web-react/issues/{{id}}"
        },
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}",
      "requreCleanWorkDir": false,
      "requreUpstream": false,
      "commitMessage": "chore(release): v${version}",
      "push": true
    },
    "gitlab": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
